import java.util.ArrayList;
import java.util.List;

public class Day1006_02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		long[] a =solution(-4,2);
		for(Long i:a) {
			System.out.println(i);
		}

	}
	public static long[] solution(int x,int n) {
		long[] ans = new long[n];
		int a=x;
		for(int i=0;i<n;++i) {
			ans[i]=(long)x;
			x+=a;
		}
		return ans;
		
	}

}
