import java.util.ArrayList;
import java.util.List;

public class Day1006_02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Long[] a =solution(2,5);
		for(Long i:a) {
			System.out.println(i);
		}

	}
	public static Long[] solution(int x,int n) {
		Long[] ans = new Long[n];
		int a=x;
		for(int i=0;i<n;++i) {
			ans[i]=(long)x;
			x+=a;
		}
		return ans;
		
	}

}
