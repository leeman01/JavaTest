/*
 * [문제] 구구단 2단에서 9단까지 모두 출력
 */
public class Exam_23 {

	public static void main(String[] args) {
		
		for (int i=1;i<=9;++i) {
			for ( int dan =2; i <= 5;++i) {
					System.out.printf("%d X %d = %2d\t",dan,i,(dan*i));
		}
		System.out.println();
	}	
	System.out.println();
		for (int i=1;i<=9;++i) {
			for ( int dan =6; i <= 5;++i) {
					System.out.printf("%d X %d = %2d\t",dan,i,(dan*i));
		}
		System.out.println();
	}	
	System.out.println();	
			
				
				
	}
	
}
		
		


